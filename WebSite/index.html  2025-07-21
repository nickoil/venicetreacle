    <script>

        if ('serviceWorker' in navigator) {
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('/sw.js');
            });
        }

        let deferredPrompt;

        window.addEventListener('beforeinstallprompt', (e) => {
            // Prevent the mini-infobar from appearing on mobile
            e.preventDefault();
            // Stash the event so it can be triggered later.
            deferredPrompt = e;
            // Update UI notify the user they can install the PWA
            const installButton = document.getElementById('install-button');
            installButton.hidden = false;

            installButton.addEventListener('click', (e) => {
                // hide our user interface that shows our A2HS button
                installButton.hidden = true;
                // Show the prompt
                deferredPrompt.prompt();
                // Wait for the user to respond to the prompt
                deferredPrompt.userChoice.then((choiceResult) => {
                    if (choiceResult.outcome === 'accepted') {
                    console.log('User accepted the A2HS prompt');
                    } else {
                    console.log('User dismissed the A2HS prompt');
                    }
                    deferredPrompt = null;
                });
            });
        });

        window.onload = function() {

            const pageContainer = document.getElementById('page-container');
            const spaceAboveTarget = 50;

            document.querySelectorAll('.vt-nav-link').forEach(link => {
                link.addEventListener('click', function(event) {

                    let scrollSpeed = 50;

                    const href= this.getAttribute('href');
                    if(href === '#') {
                        return;
                    }
                    if(href.charAt(0) !== '#') {
                        return;
                    }
                    event.preventDefault();
                    const targetId = href.substring(1);
                    const targetElement = document.getElementById('target-' + targetId);
                    if (targetElement) {
                        const scrollHeight = targetElement.offsetTop - spaceAboveTarget;
                        let scrollPosition = pageContainer.scrollTop;
                        //console.log(scrollPosition);
                        const scrollInterval = setInterval(() => {
                            scrollPosition += scrollSpeed;
                            window.scrollTo({ top: scrollPosition, behavior: "smooth" });
                            pageContainer.scrollTo(0, scrollPosition);
                            //console.log(pageContainer.scrollTop);
                            if (scrollPosition >= scrollHeight) {
                                clearInterval(scrollInterval);
                            }
                            else {
                                //approach the target slowly
                                if (scrollPosition >= scrollHeight - 50){
                                    scrollSpeed = 10;
                                }
                            }
                        }, 10); // adjust the interval time here
                    }
                });
            });

            let installMode = false;
            const urlParams = new URLSearchParams(window.location.search);
            installMode = urlParams.has('install');

            function isSafari() {
                //console.log(navigator.userAgent);
                //console.log(/^((?!chrome|android).)*safari/i.test(navigator.userAgent));
                return /^((?!chrome|android).)*safari/i.test(navigator.userAgent);
            }

            if(installMode && !isSafari()) {
                //show the install button container
                document.getElementById('install-button-container').style.display = 'block';
            }
        }

        document.addEventListener("DOMContentLoaded", function () {
            let player = videojs("mmi-stagnes");
            player.ready(function () {
                player.on("loadedmetadata", function () {
                    player.currentTime(11.8);
                });
            });

            const posterGrid = document.getElementById("poster-grid");
            const pageContainer = document.getElementById("main-content");
            const navLinks = document.querySelectorAll(".vt-nav-link");

            // Show the gig grid and hide the rest of the content
            function showPosterGrid() {
                posterGrid.style.display = "block";
                pageContainer.style.display = "none";
            }

            // Show the rest of the content and hide the gig grid
            function showPageContent() {
                posterGrid.style.display = "none";
                pageContainer.style.display = "block";
            }

            // Add event listeners to menu items
            navLinks.forEach(link => {
                link.addEventListener("click", function (event) {
                    const href = this.getAttribute("href");
                    showPageContent();
                });
            });

            document.getElementById('previous-gigs').addEventListener('click', function (event) {
                event.preventDefault();
                showPosterGrid();
                checkWidthAndToggleMasonry();
                window.scrollTo({ top: 0, behavior: "smooth" });
            });

            const masonryContainer = document.querySelector('.masonry-gallery');
            let msnry = null;

            function enableMasonry() {

                if (!msnry) {
                    msnry = new Masonry(masonryContainer, {
                        itemSelector: 'img',
                        columnWidth: '.masonry-gallery img',
                        gutter: 10,
                        fitWidth: true,
                        /*horizontalOrder: true*/
                    });
                }
            }

            function disableMasonry() {
                if (msnry) {
                    msnry.destroy();
                    msnry = null;
                }
            }

            function checkWidthAndToggleMasonry() {
                if (window.innerWidth <= 600) {
                    disableMasonry();
                } else {
                    enableMasonry();
                }
            }

            // Wait for images to load first
            imagesLoaded(masonryContainer, function () {
                // do this on showGigGrid checkWidthAndToggleMasonry();
                window.addEventListener('resize', checkWidthAndToggleMasonry);
                window.addEventListener("orientationchange", checkWidthAndToggleMasonry);
            });


            /*
            // Ensure Masonry recalculates layout after all images are loaded
            const images = masonryGallery.querySelectorAll("img");
            Promise.all(
                Array.from(images).map((img) => {
                    console.log(img.src);
                    return new Promise((resolve) => {
                        if (img.complete) {
                            resolve();
                        } else {
                            img.onload = resolve;
                            img.onerror = resolve;
                        }
                    });
                })
            ).then(() => {
                masonry.layout();
            });
*/
        });

    </script>
